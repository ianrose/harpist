<div class="gutter--vertical-2x">
	<footer class="credits">
		<h2 class="credits__heading">Credits</h2>
	  <% for(handle in authors) { var author = authors[handle] %>
	  <p>
	    <strong><%= author.name %></strong> <%= author.role %><br>
	    <% if(author.twitter) { %> @<%= author.twitter %><br> <% } %>
	  </p>
	  <% } %>
	</footer>
</div>
